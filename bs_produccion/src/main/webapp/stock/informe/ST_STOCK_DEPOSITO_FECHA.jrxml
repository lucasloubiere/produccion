	<parameter name="FCHHAS" class="java.util.Date"/>
	<parameter name="DEPOSI" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
		st_movimiento_item.DEPOSI,
		st_deposito.DESCRIPCION as st_deposito_DESCRP,
		st_movimiento_item.ARTCOD,
		st_producto.DESCRIPCION AS st_producto_DESCRP ,
		st_producto.UNIMED,
		Sum(st_movimiento_item.STOCK) AS CANTIDAD,
		ad_empresa.*
FROM 	st_movimiento_item,
		st_producto,
		st_deposito,
		ad_empresa

WHERE	st_movimiento_item.ARTCOD = st_producto.CODIGO
and st_movimiento_item.DEPOSI = st_deposito.CODIGO
And (st_movimiento_item.FCHMOV <= $P{FCHHAS} )


GROUP BY
		st_movimiento_item.DEPOSI,
		st_movimiento_item.ARTCOD,
		st_producto.DESCRIPCION,
		st_producto.UNIMED]]>
	</queryString>
