	<parameter name="FCHHHAS" class="java.sql.Timestamp"/>
	<queryString>
		<![CDATA[SELECT
		i.ARTCOD,
		st_producto.DESCRIPCION AS st_producto_DESCRP ,
		st_producto.UNIMED,
		DATE_ADD($P{FCHHHAS}, INTERVAL -1 DAY) AS FECHA,

    	(SELECT IFNULL(Sum(i1.STOCK),0) FROM st_movimiento_item i1  WHERE i1.artcod = i.artcod and  i1.FCHMOV <= DATE_ADD($P{FCHHHAS}, INTERVAL -1 DAY)) as INICIAL,


		(SELECT IFNULL(Sum(i1.STOCK),0) FROM st_movimiento_item i1
		 WHERE i1.stock > 0
		 AND i1.artcod = i.artcod
		 And i1.FCHMOV > DATE_ADD($P{FCHHHAS}, INTERVAL -1 DAY) And i1.FCHMOV < $P{FCHHHAS}) as INGRESOS,

		(SELECT IFNULL(Sum(i1.STOCK),0) FROM st_movimiento_item i1
		 WHERE i1.stock < 0
		 AND i1.artcod = i.artcod
		 And i1.FCHMOV > DATE_ADD($P{FCHHHAS}, INTERVAL -1 DAY) And i1.FCHMOV < $P{FCHHHAS}) as EGRESOS,

		 Sum(i.STOCK) AS CANTIDAD,

		ad_empresa.*
FROM 	st_movimiento_item i,
		st_producto,
		st_deposito,
		zz_temporal,
		ad_empresa

WHERE	i.ARTCOD = st_producto.CODIGO
and i.DEPOSI = st_deposito.CODIGO
and zz_temporal.codigo = i.artcod
And (i.FCHMOV <= $P{FCHHHAS})
GROUP BY i.ARTCOD, st_producto.DESCRIPCION, st_producto.UNIMED
ORDER BY i.ARTCOD, i.DEPOSI]]>
	</queryString>
